//https://practice.geeksforgeeks.org/problems/check-if-a-number-is-power-of-another-number/0/

#include <bits/stdc++.h>
using namespace std;

int main() {
    
    
    int t ; 
    cin >> t ; 
    while ( t-- )
    {
        int x , y ; 
        cin >> x >> y ; 
        double p = log2( y * 1.0 ) / log2 ( x * 1.0 ) ; 
        
        int q = log2( y ) / log2 ( x ) ; 
        if ( p - q == 0 )
            cout << 1  << endl ; 
        else
            cout << 0 << endl ; 
    }
	
	
	return 0;
}
